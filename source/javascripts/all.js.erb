var load_disqus = function(){
  var disqus_shortname="<%= site_config[:disqus_shortname] %>";(function(){var e=document.createElement("script");e.type="text/javascript";e.async=true;e.src="//"+disqus_shortname+".disqus.com/embed.js";(document.getElementsByTagName("head")[0]||document.getElementsByTagName("body")[0]).appendChild(e)})();
}

try{
  document.getElementById('load_comments').addEventListener('click', function(){
    load_disqus();
    this.remove();
  });
}catch(e){

}
